{
  "name": "project",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start:dev": "webpack serve --env port=3000",
    "start": "npm-run-all -p start:dev \"scss-definitions -- --watch\"",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "lint": "npm-run-all scss-definitions --parallel prettier eslint:ts stylelint tsc find-unused-code",
    "lint:fix": "npm-run-all scss-definitions prettier:fix eslint:ts:fix stylelint:fix tsc find-unused-code",
    "scss-definitions": "rimraf --glob \"./src/**/*.scss.d.ts\" & typed-scss-modules \"./src/**/!(var|themes)/*.scss\" --nameFormat dashes --logLevel error --additionalData \"@use './src/app/css/var/var' as *;\"",
    "eslint:ts": " eslint \"**/*.{ts,tsx}\"",
    "eslint:ts:fix": " eslint \"**/*.{ts,tsx}\" --fix",
    "prettier": "prettier --check . --loglevel warn",
    "prettier:fix": "npm run prettier -- --write . ",
    "stylelint": "stylelint \"./src/**/*.scss\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "tsc": "tsc --incremental --tsBuildInfoFile './node_modules/.cache/.tsbuildinfo'",
    "tsc:watch": "npm run tsc -- --watch",
    "find-unused-code": "ts-prune --error --ignore \"webpack.config.ts|jest.config.ts|-export|Keyframe\"",
    "test:unit": "jest --config='./config/jest/jest.config.ts'",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "build-storybook": "storybook build -c ./config/storybook"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
